{
    "name": "neos/contentrepository-development-collection",
    "description": "Event sourced Content Repository Neos packages in a joined repository for easier development.",
    "license": [
        "GPL-3.0-or-later"
    ],
    "type": "neos-package-collection",
    "require": {
        "doctrine/dbal": "*",
        "doctrine/migrations": "*",
        "neos/neos": "*",
        "neos/neos-ui": "*",
        "neos/event-sourcing": "^2.0",
        "neos/content-repository": "*",
        "neos/content-repository-dimensionspace": "*"
    },
    "replace": {
        "neos/contentgraph-doctrinedbaladapter": "self.version",
        "neos/contentgraph-postgresqladapter": "self.version",
        "neos/eventsourcedcontentrepository-legacyapi": "self.version",
        "neos/eventsourcedcontentrepository-security": "self.version",
        "neos/event-sourced-content-repository": "self.version",
        "neos/event-sourced-neos-adjustments": "self.version",
        "neos/contentrepository-intermediary": "self.version"
    },
    "suggest": [],
    "autoload": {
        "psr-4": {
            "Neos\\ContentGraph\\DoctrineDbalAdapter\\": [
                "Neos.ContentGraph.DoctrineDbalAdapter/Classes"
            ],
            "Neos\\ContentGraph\\PostgreSQLAdapter\\": [
                "Neos.ContentGraph.PostgreSQLAdapter/Classes"
            ],
            "Neos\\EventSourcedContentRepository\\LegacyApi\\": [
                "Neos.EventSourcedContentRepository.LegacyApi/Classes"
            ],
            "Neos\\EventSourcedContentRepository\\Security\\": [
                "Neos.EventSourcedContentRepository.Security/Classes"
            ],
            "Neos\\EventSourcedContentRepository\\": [
                "Neos.EventSourcedContentRepository/Classes"
            ],
            "Neos\\EventSourcedNeosAdjustments\\": [
                "Neos.EventSourcedNeosAdjustments/Classes"
            ],
            "Neos\\ContentRepository\\Intermediary\\": [
                "Neos.ContentRepository.Intermediary/Classes"
            ]
        }
    },
    "extra": {
        "installer-name": "CR",
        "neos": {
            "warning": "AUTOGENERATED FILE, ONLY MODIFY THE .composer.json IN THIS DIRECTORY AND RUN THE COMPOSER MANIFEST MERGER SCRIPT PROVIDED BY THE BUILD ESSENTIALS PACKAGE."
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neos\\EventSourcedContentRepository\\LegacyApi\\Tests\\": [
                "Neos.EventSourcedContentRepository.LegacyApi/Tests"
            ],
            "Neos\\EventSourcedContentRepository\\Tests\\": [
                "Neos.EventSourcedContentRepository/Tests"
            ],
            "Neos\\ContentGraph\\PostgreSQLAdapter\\Tests\\": [
                "Neos.ContentGraph.PostgreSQLAdapter/Tests"
            ],
            "Neos\\ContentRepository\\Intermediary\\Tests\\": [
                "Neos.EventSourcedContentRepository/Tests"
            ]
        }
    }
}
