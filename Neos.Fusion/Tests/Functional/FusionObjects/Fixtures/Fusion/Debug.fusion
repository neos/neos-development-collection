prototype(Neos.Fusion:Value).@class = 'Neos\\Fusion\\FusionObjects\\ValueImplementation'
prototype(Neos.Fusion:Debug) {
	@class = 'Neos\\Fusion\\FusionObjects\\DebugImplementation'
	plaintext = TRUE
}

debug.empty = Debug

debug.null = Debug {
	value = NULL
}

debug.nullWithTitle = Debug {
	title = 'Title'
	value = NULL
}

debug.eelExpression = Debug {
	value = ${'hello' + ' ' + 'world'}
}

debug.fusionObjectExpression = Debug {
	value = Value {
		value = 'hello world'
	}
}

debug.multipleValues = Debug {
	foo = 'foo'
	bar = 'bar'
}

debug.consoleOutput = Debug {
	title = 'helloWorld'
	value = ${'hello world'}
	console = true
}

debug.consoleOutputWithSeparator = Debug {
	title = 'withSeparator'
	value = ${'hello world'}
	console = true
	separator = ':::'
}

debug.consoleOutputWithMaxDepth = Debug {
	title = 'withMaxDepth'
	value = ${Configuration}
	console = true
	maxDepth = 0
}
