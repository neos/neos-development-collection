	// Use an exception to have the exception handler working
	20 = Neos.Fusion:Collection {
		collection = ${['Foo']}

		itemName = 'item'
		itemRenderer = Neos.Fusion:Testing.Throwing {
			shouldThrow = ${throwException}

			@cache {
