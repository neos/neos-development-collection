contentCache.nestedCacheSegmentsWithConditionalException = Neos.Fusion:Array {

	5 = 'Cached segment|'

	// This is just a cached path
	15 = Neos.Fusion:Array {
		5 = ${'counter=' + object.counter + '|'}

		@cache {
			mode = 'cached'

			entryIdentifier {
				object = ${object}
			}
		}
