{
  "name": "@neos-project/neos",
  "version": "7.1.0",
  "description": "Dependencies for building neos cms none react backend",
  "license": "GNU GPLv3",
  "scripts": {
    "build": "yarn build:js && yarn build:styles",
    "build-watch": "yarn build --progress --watch",
    "build:production": "yarn build:js:prod && yarn build:styles:prod",
    "build:js": "webpack --mode development --config=webpack.js.config.js",
    "build:styles": "webpack --mode development --config=webpack.styles.config.js",
    "build:js:prod": "webpack --mode production --config=webpack.js.config.js",
    "build:styles:prod": "webpack --mode production --config=webpack.styles.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "babel-loader": "^8.2.2",
    "compass-mixins": "^0.12.10",
    "css-loader": "^5.1.1",
    "file-loader": "^6.2.0",
    "ignore-emit-webpack-plugin": "^2.0.6",
    "mini-css-extract-plugin": "^1.3.9",
    "sass": "^1.69.5",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.4.2",
    "ts-migrate": "^0.1.35",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "~5.74.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "dompurify": "^2.2.6",
    "i18next": "^19.9.2",
    "jquery": "^3.6.0"
  }
}
