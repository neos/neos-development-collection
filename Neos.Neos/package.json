{
  "name": "@neos-project/neos",
  "version": "5.0.0",
  "description": "Dependencies for building neos cms none react backend",
  "license": "GNU GPLv3",
  "scripts": {
    "build": "yarn build:js && yarn build:styles",
    "build-watch": "yarn build --progress --colors --watch",
    "build:production": "NODE_ENV=production yarn build:js --mode production && NODE_ENV=production yarn build:styles --mode production",
    "build:js": "webpack -p --env=js",
    "build:styles": "webpack -d --env=styles",
    "build:legacy-js": "cd Scripts && npx grunt build-js"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "babel-loader": "^8.0.6",
    "compass-mixins": "^0.12.10",
    "css-loader": "^3.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "file-loader": "^4.1.0",
    "jquery": "^3.4.1"
  }
}
